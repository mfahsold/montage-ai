{
  "id": "travel",
  "name": "Travel Vlog",
  "description": "Cinematic travel style with scenic wide shots and smooth transitions",
  "params": {
    "style": {"name": "travel", "mood": "adventurous"},
    "pacing": {
      "speed": "medium",
      "variation": "high",
      "intro_duration_beats": 4,
      "climax_intensity": 0.8,
      "min_cut_beats": 2,
      "max_cut_beats": 8
    },
    "cinematography": {
      "prefer_wide_shots": true,
      "prefer_high_action": false,
      "match_cuts_enabled": true,
      "invisible_cuts_enabled": true,
      "shot_variation_priority": "maximum"
    },
    "transitions": {
      "type": "mixed",
      "crossfade_duration_sec": 0.5,
      "whip_pan_probability": 0.1
    },
    "effects": {
      "color_grading": "cinematic_teal_orange",
      "stabilization": true,
      "sharpness_boost": false,
      "vignette": true
    },
    "energy_mapping": {
      "sync_to_beats": true,
      "energy_amplification": 1.0,
      "beat_reactivity": "moderate"
    },
    "story_arc": {
      "preset": "slow_burn",
      "location_variety": true
    }
  }
}
